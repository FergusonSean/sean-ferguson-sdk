{
  "name": "@seanferguson/the-one-sdk",
  "version": "0.1.0",
  "description": "An sdk for accessing LOTR data through https://the-one-api.dev/",
  "scripts": {
    "build": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint 'src/**/*.[tj]s' 'test/**/*.test.[tj]s'",
    "lint:fix": "eslint --fix . 'src/**/*.[tj]s' 'test/**/*.[tj]s'",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.7",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^10.2.0",
    "prettier": "^2.5.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
